{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false,
    "sourceMap": true,
    "outDir": "./build",
    "rootDir": "./src",
    "moduleResolution": "node",
    "baseUrl": ".",
  }
}
